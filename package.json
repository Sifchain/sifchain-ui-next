{
  "name": "sifchain",
  "version": "0.0.0",
  "private": true,
  "description": "",
  "scripts": {
    "prepare": "husky install",
    "build": "turbo run build",
    "build:pkgs": "turbo run build --filter=\"./packages/*\"",
    "test": "turbo run test",
    "dev": "turbo run dev --filter=@sifchain/dex",
    "dev:registry": "turbo run dev --filter=@sifchain/registry",
    "lint": "turbo run lint",
    "compile": "turbo run compile",
    "format": "prettier --write .",
    "storybook": "pnpm -C ./packages/ui run storybook ",
    "build-storybook": "turbo run build-storybook",
    "docs:dev": "pnpm -C ./apps/docs dev",
    "e2e": "pnpm -C ./apps/e2e run test"
  },
  "keywords": [],
  "license": "Apache-2.0",
  "devDependencies": {
    "@changesets/cli": "^2.24.2",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "eslint": "^8.22.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "rimraf": "^3.0.2",
    "turbo": "^1.4.3",
    "typescript": "4.7.4"
  }
}
